{
  "name": "advent-of-code-2023",
  "version": "1.0.0",
  "description": "Solutions for Advent of Code 2023",
  "main": "index.js",
  "scripts": {
    "day1": " npx tsc --lib 'ES2022' --outDir ./day1/dist/ ./day1/index.ts && cp ./day1/input.txt ./day1/dist && node ./day1/dist/index.js",
    "day2": " npx tsc --lib 'ES2022' --outDir ./day2/dist/ ./day2/index.ts && cp ./day2/input.txt ./day2/dist && node ./day2/dist/index.js",
    "day3": " npx tsc --lib 'ES2022' --outDir ./day3/dist/ ./day3/index.ts && cp ./day3/input.txt ./day3/dist && node ./day3/dist/index.js",
    "day4": " npx tsc --lib 'ES2022' --outDir ./day4/dist/ ./day4/index.ts && cp ./day4/input.txt ./day4/dist && node ./day4/dist/index.js",
    "test:day1": "jest -t day1",
    "test:day2": "jest -t day2"
  },
  "author": "Wolfgang Kreminger",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.4.0-dev.20231201"
  },
  "dependencies": {
    "@types/node": "^18.11.10",
    "babel-runtime": "^6.26.0",
    "numjs": "^0.16.1",
    "pandas-js": "^0.2.4"
  }
}
